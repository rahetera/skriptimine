#!/bin/bash
echo -n "K端psetusplaadi laius: "
read laius
echo -n "k端psetusplaadi pikkus: "
read pikkus
echo -n "K端psise pindala(cm2): "
read kupsis
echo -n "Kihtide arv: "
read kihte
echo -n "K端psiseid pakis: "
read pakke
plaat=`expr $laius \* $pikkus`
suurus=`expr $plaat / $kupsis`
kihtidearv=`expr $suurus \* $kihte`
vpakke=`expr $kihtidearv / $pakke`
echo "Kokku on vaja $vpakke pakki"
#
